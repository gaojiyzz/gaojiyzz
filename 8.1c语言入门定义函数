#include<stdio.h>
int main()
{
int max(int x,int y);      //对被调用函数max函数的声明
int a,b,c;
scanf("%d,%d"a,b);
c=max(a,b);                //调用max函数，将得到的值赋给c
printf("max=%d",c);
return 0;
}

int max(int x,int y)       //定义max函数，函数值为整数，形式参数x和y为整形
{
int z;                     //max函数中的声明部分，定义本函数中用到的变量z为整型
if(x>y)z=x;
else z=y;
return (z);                //将z的值返回，通过max带回到调用函数的位置
}
